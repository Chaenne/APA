#include <stdio.h>

void InsertionSort(int a[], int n ){

 int i, j, pivo ,aux;

 	for (i=1;i<=n-1;i++){
      pivo = a[i];
      j = i-1;
      
      while (j>= 0 && a[j] > pivo){
       a[j+1] = a[j];
        j= j-1;
      }  
   
    a[j+1] = pivo;
  }
  }
int main(){ 
	// lÃª o arquivo a ser ordenado 
	int n;
	FILE* f = fopen("teste","r");
	fscanf(f,"%d", &n);
	int arr[n];
	for(int i = 0; i < n; i++){
		fscanf(f,"%d", &arr[i]);
	}
	//SelectionSort(arr,n);
	InsertionSort(arr,n);
	for(int i = 0; i < n; i++){
		printf("%d\n",arr[i] );
	}
}
